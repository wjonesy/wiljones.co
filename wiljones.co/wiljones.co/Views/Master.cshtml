@using ClientDependency.Core.Mvc
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    //Html.RequiresCss("~/css/main.css");

    //Html.RequiresJs("~/scripts/bootstrap.min.js");
